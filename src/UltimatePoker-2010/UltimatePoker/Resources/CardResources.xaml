<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:my="clr-namespace:UltimatePoker"
	xmlns:controls="clr-namespace:UltimatePoker.Controls"
    >

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/UltimatePoker;component/Resources/CardArtTemplates.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <!--Convertes-->
    <my:CardValueConverter x:Key="CardValueConverter"/>
    <my:CardTemplateSelector x:Key="CardTemplateSelector"/>
    <my:CashConverter x:Key="CashConverter"/>
    <my:ActionNameConverter x:Key="ActionNameConverter"/>
    <my:MarginConverter x:Key="MarginConverter"/>
    <BooleanToVisibilityConverter x:Key="BooleanToVisibility"/>
    <my:ColumnConverter x:Key="ColumnConverter"/>
    <my:CardArtTemplateSelector x:Key="CardArtTemplateSelector"/>

    <!--Default Colors-->
    <SolidColorBrush x:Key="CardBlack">
        <SolidColorBrush.Color>#FF000000</SolidColorBrush.Color>
    </SolidColorBrush>
    <SolidColorBrush x:Key="CardRed">
        <SolidColorBrush.Color>#FFC50000</SolidColorBrush.Color>
    </SolidColorBrush>

    <!--Defines a default style for the application buttons-->
    <Style TargetType="{x:Type Button}">
        <Setter Property="OverridesDefaultStyle" Value="True"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Grid>
                        <Rectangle StrokeThickness="2" RadiusX="4" RadiusY="4" x:Name="rectangle">
                            <Rectangle.Fill>
                                <LinearGradientBrush EndPoint="0.482,1.278" StartPoint="0.482,0.022">
                                    <GradientStop Color="#FF02811A" Offset="0.01"/>
                                    <GradientStop Color="#FF150F00" Offset="0.75"/>
                                </LinearGradientBrush>
                            </Rectangle.Fill>
                            <Rectangle.Stroke>
                                <LinearGradientBrush EndPoint="0.479,0.951" StartPoint="0.479,-0.12">
                                    <GradientStop Color="#FFFFDA00" Offset="0"/>
                                    <GradientStop Color="#FF242424" Offset="1"/>
                                </LinearGradientBrush>
                            </Rectangle.Stroke>
                        </Rectangle>
                        <ContentPresenter HorizontalAlignment="Center"
															VerticalAlignment="Center"/>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <EventTrigger RoutedEvent="Mouse.MouseEnter">
                            <EventTrigger.Actions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="rectangle" Storyboard.TargetProperty="(Shape.Fill).(GradientBrush.GradientStops)[1].(GradientStop.Offset)">
                                            <SplineDoubleKeyFrame KeyTime="00:00:00.1000000" Value="1"/>
                                        </DoubleAnimationUsingKeyFrames>
                                        <ColorAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="rectangle" Storyboard.TargetProperty="(Shape.Fill).(GradientBrush.GradientStops)[0].(GradientStop.Color)">
                                            <SplineColorKeyFrame KeyTime="00:00:00.1000000" Value="#FF03D52B"/>
                                        </ColorAnimationUsingKeyFrames>
                                    </Storyboard>
                                </BeginStoryboard>
                            </EventTrigger.Actions>
                        </EventTrigger>
                        <EventTrigger RoutedEvent="Mouse.MouseLeave">
                            <EventTrigger.Actions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="rectangle" Storyboard.TargetProperty="(Shape.Fill).(GradientBrush.GradientStops)[1].(GradientStop.Offset)">
                                            <SplineDoubleKeyFrame KeyTime="00:00:00.1000000" Value="0.76"/>
                                        </DoubleAnimationUsingKeyFrames>
                                        <ColorAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="rectangle" Storyboard.TargetProperty="(Shape.Fill).(GradientBrush.GradientStops)[0].(GradientStop.Color)">
                                            <SplineColorKeyFrame KeyTime="00:00:00.1000000" Value="#FF02811A"/>
                                        </ColorAnimationUsingKeyFrames>
                                    </Storyboard>
                                </BeginStoryboard>
                            </EventTrigger.Actions>
                        </EventTrigger>

                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="FontWeight" Value="Bold"/>
                        </Trigger>

                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Fill" TargetName="rectangle" Value="#FF02811A"/>
                        </Trigger>

                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Fill" TargetName="rectangle">
                                <Setter.Value>
                                    <LinearGradientBrush EndPoint="0.482,1.278" StartPoint="0.482,0.022">
                                        <GradientStop Color="#FF4F8259" Offset="0.01"/>
                                        <GradientStop Color="#FF150F00" Offset="0.75"/>
                                    </LinearGradientBrush>
                                </Setter.Value>
                            </Setter>
                            <Setter Property="Stroke" TargetName="rectangle">
                                <Setter.Value>
                                    <LinearGradientBrush EndPoint="0.479,0.951" StartPoint="0.479,-0.12">
                                        <GradientStop Color="#FFD3C886" Offset="0.014"/>
                                        <GradientStop Color="#FF242424" Offset="1"/>
                                    </LinearGradientBrush>
                                </Setter.Value>
                            </Setter>
                            <Setter Property="Foreground" Value="#FF979797"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Foreground" Value="White"/>
    </Style>

    <!--Defines the control template for the front of the card-->
    <ControlTemplate x:Key="CardFrontControlTemplate">
        <Grid Margin="{Binding Path=IsSelected,Converter={StaticResource MarginConverter}}"
                          Width="121.892" Height="159.752">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="0.061*"/>
                <ColumnDefinition Width="0.165*"/>
                <ColumnDefinition Width="0.564*"/>
                <ColumnDefinition Width="0.167*"/>
                <ColumnDefinition Width="0.043*"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="0.038*"/>
                <RowDefinition Height="0.113*"/>
                <RowDefinition Height="0.132*"/>
                <RowDefinition Height="0.429*"/>
                <RowDefinition Height="0.134*"/>
                <RowDefinition Height="0.121*"/>
                <RowDefinition Height="0.033*"/>
            </Grid.RowDefinitions>
            <Grid.BitmapEffect>
                <DropShadowBitmapEffect Direction="185" Opacity="0.7" ShadowDepth="3"/>
            </Grid.BitmapEffect>
            <Rectangle Fill="#FFFFFFFF" Stroke="#FF000000" RadiusX="14.5" RadiusY="14.5" x:Name="CardBorder" Grid.ColumnSpan="5" Grid.RowSpan="7"
											 Visibility="{Binding Path=IsSelected,Converter={StaticResource BooleanToVisibility},FallbackValue=Hidden}">
                <Rectangle.BitmapEffect>
                    <OuterGlowBitmapEffect GlowSize="9" GlowColor="#FF1144FF"/>
                </Rectangle.BitmapEffect>
            </Rectangle>
            <Rectangle Fill="#FFFFFFFF" Stroke="#FF000000" RadiusX="14.5" RadiusY="14.5" Grid.ColumnSpan="5" Grid.RowSpan="7" x:Name="CardBG"/>
            <TextBlock FontSize="15" FontWeight="Bold" Text="{Binding Path=Card.CardValue,Converter={StaticResource CardValueConverter}}"
					   TextWrapping="Wrap" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Row="1" x:Name="CardTopText"/>
			<controls:SuiteControl Grid.Row="2" Grid.Column="1" x:Name="CardTopSuite" Content="{Binding Path=Card}"/>
			<TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Column="3" Grid.Row="5" FontSize="15" FontWeight="Bold" TextWrapping="Wrap" RenderTransformOrigin="0.5,0.5"
						 Text="{Binding Path=Card.CardValue,Converter={StaticResource CardValueConverter}}" x:Name="CardBottomText">
				<TextBlock.RenderTransform>
					<TransformGroup>
						<ScaleTransform ScaleX="-1" ScaleY="-1"/>
						<SkewTransform AngleX="0" AngleY="0"/>
						<RotateTransform Angle="0"/>
						<TranslateTransform X="0" Y="0"/>
					</TransformGroup>
				</TextBlock.RenderTransform>
			</TextBlock>
			<controls:SuiteControl Grid.Column="3" Grid.Row="4" RenderTransformOrigin="0.5,0.5" x:Name="CardBottomSuite" Content="{Binding Path=Card}">
				<controls:SuiteControl.RenderTransform>
					<TransformGroup>
						<ScaleTransform ScaleX="-1" ScaleY="-1"/>
						<SkewTransform AngleX="0" AngleY="0"/>
						<RotateTransform Angle="0"/>
						<TranslateTransform X="0" Y="0"/>
					</TransformGroup>
				</controls:SuiteControl.RenderTransform>
			</controls:SuiteControl>
			<ContentControl x:Name="CardArtHolder" Grid.Row="1" Grid.RowSpan="5" Grid.Column="2"
							ContentTemplateSelector="{StaticResource CardArtTemplateSelector}" Content="{Binding Path=Card}"/>
			<ContentPresenter/>
		</Grid>
        <!--Sets colors and symbols according to suite-->
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding Path=Card.CardSuite}" Value="Spades">
                <Setter TargetName="CardTopText" Property="Foreground" Value="{StaticResource CardBlack}"/>
                <Setter TargetName="CardBottomText" Property="Foreground" Value="{StaticResource CardBlack}"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=Card.CardSuite}" Value="Hearts">
                <Setter TargetName="CardTopText" Property="Foreground" Value="{StaticResource CardRed}"/>
                <Setter TargetName="CardBottomText" Property="Foreground" Value="{StaticResource CardRed}"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=Card.CardSuite}" Value="Diamonds">
                <Setter TargetName="CardTopText" Property="Foreground" Value="{StaticResource CardRed}"/>
                <Setter TargetName="CardBottomText" Property="Foreground" Value="{StaticResource CardRed}"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=Card.CardSuite}" Value="Clobes">
                <Setter TargetName="CardTopText" Property="Foreground" Value="{StaticResource CardBlack}"/>
                <Setter TargetName="CardBottomText" Property="Foreground" Value="{StaticResource CardBlack}"/>
            </DataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <DataTemplate x:Key="CardFrontTemplate" DataType="{x:Type my:CardWrapper}">
        <Button x:Name="CardButton" Template="{StaticResource CardFrontControlTemplate}"/>
    </DataTemplate>

    <!--Defines the way cards are displayed one over the other-->
    <Style x:Key="CardsItemsControl" TargetType="{x:Type ItemsControl}" BasedOn="{StaticResource {x:Type ItemsControl}}">
        <Setter Property="Panel.ZIndex" Value="1"/>
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <Grid HorizontalAlignment="Stretch">
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="1*"/>
                        </Grid.ColumnDefinitions>
                    </Grid>
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ItemContainerStyle">
            <Setter.Value>
                <Style TargetType="{x:Type ContentPresenter}" >
                    <Setter Property="Grid.ColumnSpan" Value="4"/>
                    <Setter Property="Grid.Column">
                        <Setter.Value>
                            <MultiBinding Converter="{StaticResource ColumnConverter}">
                                <Binding RelativeSource="{RelativeSource Mode=FindAncestor,AncestorType={x:Type ItemsControl}}"/>
                                <Binding RelativeSource="{x:Static RelativeSource.Self}"/>
                            </MultiBinding>
                        </Setter.Value>
                    </Setter>
                </Style>
            </Setter.Value>
        </Setter>
    </Style>

